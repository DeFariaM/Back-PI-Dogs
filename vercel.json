{
  "version": 2,

  "builds": [{ "src": "./index.js", "use": "@vercel/node" }],

  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],

  "functions": {
    "api/hello.js": {
      "memory": 3008,
      "maxDuration": 60
    },
    "api/another.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
